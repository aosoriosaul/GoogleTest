<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="dataset-final">
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>add-cors</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPTargetConnection>
    <URL>https://healthcare.googleapis.com/v1/{gcp.healthcareapi-path}/fhir</URL>
    <Authentication>
      <GoogleAccessToken>
        <Scopes>
          <Scope>https://www.googleapis.com/auth/cloud-platform</Scope>
        </Scopes>
      </GoogleAccessToken>
    </Authentication>
  </HTTPTargetConnection>
</TargetEndpoint>
