<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MessageLogging continueOnError="false" enabled="true" name="ML-saveip-region">
  <DisplayName>ML-saveip-region</DisplayName>
  <CloudLogging>
    <LogName>projects/apigee-prod-370323/logs/apigee-dev-logger</LogName>
    <Message contentType="application/json">{
    "logName": "projects/{organization.name}/logs/dev-logger",
    "message": "Displaying logs from test-ip",
    "client_region": {request.header.x-request-client-region},
    "client_ip": {request.header.x-client-ip-address},
    "client-city": {request.header.x-request-client-city}
    }
    </Message>
    <Labels>
      <Label>
        <Key>Service</Key>
        <Value>Apigee</Value>
      </Label>
    </Labels>
    <ResourceType>api</ResourceType>
  </CloudLogging>
  <logLevel>INFO</logLevel>
</MessageLogging>
